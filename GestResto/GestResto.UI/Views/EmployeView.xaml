<UserControl x:Class="GestResto.UI.Views.EmployeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="600" Width="1000">
    <Grid>
        <Label x:Name="lblTitre" Content="Informations d'un Employé" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label x:Name="lblType" Content="Type Employé" HorizontalAlignment="Left" Margin="0,22,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="lbxListeCategorie" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Height="252" Margin="308,38,0,0" VerticalAlignment="Top" Width="186">
            <Button x:Name="btn123" Content="Vincent Desrosiers #123" Width="163" Height="30"/>
        </ListBox>
        <Label Content="Liste de tous les employés" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Margin="296,10,0,0" VerticalAlignment="Top" Width="194"/>
        <ComboBox x:Name="cbxType" HorizontalAlignment="Left" Margin="89,25,0,0" VerticalAlignment="Top" Width="64"/>
        <Label Content="Numéro" HorizontalAlignment="Left" Margin="159,21,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNum" HorizontalAlignment="Left" Height="23" Margin="218,25,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="54"/>
        <Label Content="Nom" HorizontalAlignment="Left" Margin="0,48,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNom" HorizontalAlignment="Left" Height="23" Margin="89,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label Content="Mot De Passe&#xD;&#xA;" HorizontalAlignment="Left" Margin="0,130,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtMDP" HorizontalAlignment="Left" Height="23" Margin="89,134,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" RenderTransformOrigin="0.525,-0.043"/>
        <Label Content="Prénom" HorizontalAlignment="Left" Margin="0,76,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtPrenom" HorizontalAlignment="Left" Height="23" Margin="89,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label Content="Téléphone&#xD;&#xA;" HorizontalAlignment="Left" Margin="0,104,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtTel" HorizontalAlignment="Left" Height="23" Margin="89,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label Content="Ville&#xD;&#xA;&#xA;" HorizontalAlignment="Left" Margin="0,186,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtVille" HorizontalAlignment="Left" Height="23" Margin="89,190,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" RenderTransformOrigin="0.525,-0.043"/>
        <Label Content="Adresse&#xD;&#xA;" HorizontalAlignment="Left" Margin="0,160,0,0" VerticalAlignment="Top" Height="26"/>
        <TextBox x:Name="txtAdresse" HorizontalAlignment="Left" Height="23" Margin="89,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label Content="Code Postal&#xD;&#xA;&#xA;" HorizontalAlignment="Left" Margin="0,214,0,0" VerticalAlignment="Top" Height="27"/>
        <TextBox x:Name="txtCP" HorizontalAlignment="Left" Height="23" Margin="89,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label Content="NAS&#xD;&#xA;&#xA;" HorizontalAlignment="Left" Margin="0,242,0,0" VerticalAlignment="Top" Height="27"/>
        <TextBox x:Name="txtNAS" HorizontalAlignment="Left" Height="23" Margin="89,246,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label Content="Taux Horaire&#xD;&#xA;&#xA;" HorizontalAlignment="Left" Margin="0,270,0,0" VerticalAlignment="Top" Height="27"/>
        <TextBox x:Name="txtTauxHoraire" HorizontalAlignment="Left" Height="23" Margin="89,274,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="118"/>
        <CheckBox x:Name="chkActif" Content="Actif" HorizontalAlignment="Left" Margin="228,278,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
