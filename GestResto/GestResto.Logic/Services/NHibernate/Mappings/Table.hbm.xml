<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="GestResto.Logic"
                   namespace="GestResto.Logic.Model.Entities"
                   xmlns="urn:nhibernate-mapping-2.2">
  <class name="Table" table="tables">

    <id name="IdTable">
      <column name="idTable" not-null="true" sql-type="INTEGER" />
      <generator class="identity" />
    </id>

    <property name="NoTable">
      <column name="noTable" not-null="true" sql-type="INTEGER" />
    </property>

    <property name="EstActif">
      <column name="actif" not-null="true" sql-type="BOOLEAN" />
    </property>

    <property name="EstAssigne">
      <column name="assigne" not-null="true" sql-type="BOOLEAN" />
    </property>

    <bag name="ListeClients" inverse="false" generic="true" lazy="false" cascade="all">
      <key>
        <column name="idTable" />
      </key>
      <one-to-many class="Client"/>
    </bag>
    
  </class>

</hibernate-mapping>
