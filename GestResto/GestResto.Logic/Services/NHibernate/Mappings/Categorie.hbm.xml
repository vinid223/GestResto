<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="GestResto.Logic"
                   namespace="GestResto.Logic.Model.Entities"
                   xmlns="urn:nhibernate-mapping-2.2">
  <class name="Categorie" table="categories">
    
    <id name="IdCategorie">
      <column name="idCategorie" not-null="true" sql-type="INTEGER" />
      <generator class="identity" />
    </id>
    
    <property name="Nom">
      <column name="nom" not-null="true" sql-type="VARCHAR" />
    </property>
    <property name="EstActif">
      <column name="actif" not-null="true" sql-type="TINYINT" />
    </property>
    <property name="EstComplementaire">
      <column name="complementaire" not-null="true" sql-type="TINYINT" />
    </property>
  </class>
</hibernate-mapping>