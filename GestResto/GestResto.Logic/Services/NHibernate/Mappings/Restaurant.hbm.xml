<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="GestResto.Logic"
                   namespace="GestResto.Logic.Model.Entities"
                   xmlns="urn:nhibernate-mapping-2.2">
  <class name="Restaurant" table="Restaurants">

    <id name="IdRestaurant">
      <column name="idRestaurant" not-null="true" sql-type="INTEGER" />
      <generator class="identity" />
    </id>

    <property name="Nom">
      <column name="nom" not-null="true" sql-type="VARCHAR" />
    </property>

    <property name="Adresse">
      <column name="adresse" not-null="true" sql-type="VARCHAR" />
    </property>

    <property name="Telephone">
      <column name="telephone" not-null="true" sql-type="INTEGER" />
    </property>

    <property name="Fax">
      <column name="fax" not-null="true" sql-type="INTEGER" />
    </property>

    <property name="Ville">
      <column name="ville" not-null="true" sql-type="VARCHAR" />
    </property>

    <property name="CodePostal">
      <column name="codePostal" not-null="true" sql-type="VARCHAR" />
    </property>
  </class>

</hibernate-mapping>
